@page
@model EnvironmentModel
@{
    ViewData["Title"] = "Environment";
}
<h1>@ViewData["Title"]</h1>

<div>
    <table class="table">
        <thead>
            <tr>
                <th>
                    Config
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.ConfigList)
            {
            <tr>
                <td>
                    @item.Key
                </td>
                <td>
                    @item.Value
                </td>
            </tr>
            }
        </tbody>
    </table>
</div>

